<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="stock_input_sequence" model="ir.sequence">
            <field name="name">Stock Input Rawat Inap</field>
            <field name="code">stock.input.rawat.inap</field>
            <field name="prefix">RN/%(day)s/%(month)s/%(y)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="stock_output_sequence" model="ir.sequence">
            <field name="name">Stock Output Rawat Inap</field>
            <field name="code">stock.output.rawat.inap</field>
            <field name="prefix">DO/%(day)s/%(month)s/%(y)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>